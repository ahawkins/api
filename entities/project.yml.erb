---
"$schema": http://json-schema.org/draft-04/hyper-schema
title: Project
description: A Project represents a project on SemaphoreCI.
stability: prototype
strictProperties: true
type:
- object
definitions:
  id:
    description: unique identifier of project
    readOnly: true
    format: uuid
    type:
    - string

  identity:
    anyOf:
    - "$ref": "/schemata/project#/definitions/id"

  name:
    description: unique name of project
    readOnly: true
    example: "semaphore"
    type:
    - string

  url:
    description: a link to the project in API
    example: "https://semaphoreci.com/api/internal/{project_owner_name}/{project_name}"
    readOnly: true
    type:
    - string

  default_branch_url:
    description: a link to the project's default branch
    example: "https://semaphoreci.com/api/internal/{project_owner_name}/{project_name}/{branch_name}"
    readOnly: true
    type:
    - string

  html_url:
    description: a link to the project page
    readOnly: true
    example: "https://semaphoreci.com/{project_owner_name}/{project_name}"
    type:
    - string

  settings_html_url:
    description: a link to project's settings page
    readOnly: true
    example: "https://semaphoreci.com/{project_owner_username}/{project_name}/settings"
    type:
    - string

  created_at:
    description: when project was created
    format: date-time
    type:
    - string

  updated_at:
    description: when project was updated
    format: date-time
    type:
    - string

links:
- description: Info for existing project.
  href: "/<%= identity(:owner, :project) %>"
  method: GET
  rel: self
  title: Info

- description: List existing projects.
  href: "/<%= identity(:owner) %>/projects"
  method: GET
  rel: instances
  title: List âœ–

properties:
  id:
    "$ref": "/schemata/project#/definitions/id"
  name:
    "$ref": "/schemata/project#/definitions/name"
  star:
    "$ref": "/schemata/star"
  owner:
    "$ref": "/schemata/owner"
  url:
    "$ref": "/schemata/project#/definitions/url"
  default_branch_url:
    "$ref": "/schemata/project#/definitions/default_branch_url"
  html_url:
    "$ref": "/schemata/project#/definitions/html_url"
  settings_html_url:
    "$ref": "/schemata/project#/definitions/settings_html_url"
  created_at:
    "$ref": "/schemata/project#/definitions/created_at"
  updated_at:
    "$ref": "/schemata/project#/definitions/updated_at"
id: schemata/project
