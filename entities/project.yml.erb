---
"$schema": http://json-schema.org/draft-04/hyper-schema
title: Project
description: A Project represents a project on SemaphoreCI.
stability: prototype
strictProperties: true
type:
- object
definitions:
  id:
    description: unique identifier of project
    readOnly: true
    format: uuid
    type:
    - string

  name:
    description: unique name of project
    readOnly: true
    example: "semaphore"
    type:
    - string

  identity:
    anyOf:
    - "$ref": "/schemata/project#/definitions/id"

  owner:
    description: project owner's username
    readOnly: true
    example: "renderedtext"
    type:
    - string

  starred:
    description: indicates whether the current user has starred the project
    readOnly: true
    type:
    - boolean

  default_branch:
    description: name of the project's default branch
    readOnly: true
    example: "master"
    type:
    - string

  build_state:
    description: last build status of the default branch
    readOnly: true
    example: "passed"
    type:
    - string

  message:
    description: a message regarding project's state
    readOnly: true
    example: "Master build failed."
    type:
    - string

  project_html_url:
    description: a link to the project page
    readOnly: true
    example: "https://semaphoreci.com/{project_owner_name}/{project_name}"
    type:
    - string

  owner_html_url:
    description: a link to the project's owner page
    readOnly: true
    example: "https://semaphoreci.com/{project_owner_name}"
    type:
    - string

  toggle_star_url:
    description: a link to star/unstar the project
    readOnly: true
    example: "https://semaphoreci.com/api/internal/{project_owner_username}/{project_name}/toggle_star"
    type:
    - string

  build_html_url:
    description: a link to the last build page of the default branch
    readOnly: true
    example: "https://semaphoreci.com/{project_owner_username}/{project_name}/branches/{default_branch_name}/builds/{build_id}"
    type:
    - string

  settings_html_url:
    description: a link to project's settings page
    readOnly: true
    example: "https://semaphoreci.com/{project_owner_username}/{project_name}/settings"
    type:
    - string

  created_at:
    description: when project was created
    format: date-time
    type:
    - string

  updated_at:
    description: when project was updated
    format: date-time
    type:
    - string

links: []
properties:
  id:
    "$ref": "/schemata/project#/definitions/id"
  name:
    "$ref": "/schemata/project#/definitions/name"
  created_at:
    "$ref": "/schemata/project#/definitions/created_at"
  updated_at:
    "$ref": "/schemata/project#/definitions/updated_at"
id: schemata/project
